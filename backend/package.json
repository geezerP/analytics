    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "express": "^4.17.1",
    "googleapis": "^68.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.0"
  }
}
